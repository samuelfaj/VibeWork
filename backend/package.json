{
  "name": "@vibework/backend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "test": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "elysia": "latest",
    "@elysiajs/swagger": "latest",
    "@elysiajs/cors": "latest",
    "drizzle-orm": "latest",
    "mysql2": "latest",
    "mongoose": "latest",
    "@typegoose/typegoose": "latest",
    "reflect-metadata": "latest",
    "ioredis": "latest",
    "@google-cloud/pubsub": "latest",
    "i18next": "latest"
  },
  "devDependencies": {
    "typescript": "latest",
    "@types/node": "latest",
    "vitest": "latest"
  }
}
